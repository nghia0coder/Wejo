{
  "ConnectionStrings": {
    "DefaultConnection": "Host={DbServer};Database={DbName};Port={DbPort};User Id={DbUser};Password={DbPassword}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Environment": "local",
  "SwaggerEnabled": true,
  "DevMode": true,
  "Protocols": "",
  "Origins": "http://localhost:3000"
}
